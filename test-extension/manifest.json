{
    "manifest_version": 2,
    "name": "Page link copier",
    "version": "1.0",
    "description": "Copies the link of the current page to the clipboard",
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ]  
  }
  